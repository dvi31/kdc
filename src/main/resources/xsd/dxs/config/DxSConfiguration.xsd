<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2009 sp1 (http://www.altova.com) by DICTAO (DICTAO) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Ce fichier definit le format des fichiers de configuration A2S 3.7 et AVS 4.0-->
	<!--Remarque : commencer par l'element AxSConfiguration pour avoir une vision d'ensemble-->
	<!--**************************************************************************************************************************-->
	<xs:simpleType name="zeroOuUnType">
		<xs:restriction base="xs:integer">
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="Label" type="xs:token"/>
	<xs:element name="URL" type="xs:anyURI"/>
	<xs:element name="PolicyIdentifier" type="xs:token"/>
	<xs:element name="SrvAuthCert" type="xs:string"/>
	<xs:attribute name="Reference" type="xs:anyURI"/>
	<xs:element name="IsActive" type="zeroOuUnType"/>
	<xs:complexType name="DigestMethodType" mixed="true">
		<xs:attribute name="Algorithm" use="required"/>
	</xs:complexType>
	<xs:simpleType name="DigestValueType">
		<xs:restriction base="xs:base64Binary"/>
	</xs:simpleType>
	<xs:element name="X509Certificate" type="xs:string"/>
	<xs:element name="X509SubjectName" type="xs:token"/>
	<xs:complexType name="RangeType">
		<xs:sequence>
			<xs:element name="RangeMin" type="xs:token"/>
			<xs:element name="RangeMax" type="xs:token"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PeriodType">
		<xs:sequence>
			<xs:element name="DateBegin" type="xs:date"/>
			<xs:element name="DateEnd" type="xs:date"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IdentifierType">
		<xs:simpleContent>
			<xs:extension base="xs:token">
				<xs:attribute name="Qualifier" use="optional">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="OIDAsURI"/>
							<xs:enumeration value="OIDAsURN"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="PolicyIdentifierType">
		<xs:sequence>
			<xs:element name="Identifier" type="IdentifierType"/>
			<xs:element name="SigPolicyHash">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DigestMethod" type="DigestMethodType"/>
						<xs:element name="DigestValue" type="DigestValueType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Description" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="LogicalIdentifier" type="xs:token"/>
	<xs:element name="AliasRef">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:token">
					<xs:attribute ref="Reference" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="AdminGroups">
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="GroupRef" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AllGroups"/>
				</xs:choice>
				<xs:element ref="AuthorizedOperations" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ManagerApplis">
		<xs:complexType>
			<xs:choice>
				<xs:element ref="AppliRef" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="AllApplis"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="AuthorizedOperations">
		<xs:complexType>
			<xs:choice>
				<xs:element name="OperationId" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="AllOperations"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="AppliRef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Seal">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="LastUpdate" type="xs:dateTime"/>
				<xs:element name="UpdatedBy" type="xs:token"/>
				<xs:element name="Signature" minOccurs="0">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:base64Binary">
								<xs:attribute name="Algorithm">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="http://www.w3.org/2000/09/xmldsig#rsa-sha1"/>
											<xs:enumeration value="http://www.w3.org/2001/04/xmldsig-more#rsa-sha256"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Internals">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="àéèöû"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CommitmentType" type="xs:token"/>
	<xs:simpleType name="AuthenticationFormatType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="CAP"/>
			<xs:enumeration value="CVV"/>
			<xs:enumeration value="OTPSEND"/>
			<xs:enumeration value="CERT"/>
			<xs:enumeration value="OATH"/>
			<xs:enumeration value="CSR"/>
			<xs:enumeration value="PERSONAL_DATA"/>
			<xs:enumeration value="CREDENTIAL_QA"/>
			<xs:enumeration value="LOGIN_PWD"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AuthenticationFormat" type="AuthenticationFormatType"/>
	<xs:simpleType name="ExtractionAlgorithmType">
		<xs:restriction base="xs:string">
				<xs:enumeration value="DN_SEARCH"/>
				<xs:enumeration value="DN_AND_IDN_SEARCH"/>
				<xs:enumeration value="SN_SEARCH"/>
				<xs:enumeration value="CUSTOM_SEARCH"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ExtractionMethodListType">
		<xs:sequence>
			<xs:element name="ExtractionMethod" type="ExtractionMethodType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExtractionMethodType">
		<xs:sequence>
			<xs:element name="Identifier" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Regex" type="xs:string" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<!--**************************************************************************************************************************-->
	<!--SUPER-ADMINISTRATION-->
	<!--**************************************************************************************************************************-->
	<!--Configuration de la liste des Autorites de Certification referencees-->
	<xs:element name="CRLDPList" type="CRLDPListType"/>
	<xs:element name="DCRLDPList" type="CRLDPListType"/>
	<xs:complexType name="CRLDPListType">
		<xs:sequence>
			<xs:element name="CRLDP" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="URL"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ApprovedCARef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="ExtensionIdentifierType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="id-ce-certificatePolicies.CertPolicyId"/>
			<xs:enumeration value="id-ce-extendedKeyUsage"/>
			<xs:enumeration value="id-ce-keyUsage"/>
			<!-- TODO faire une restriction de OtherExtensionType
						<xs:enumeration value="id-pe-qcStatements.id-etsi-qcs-QcCompliance"/>
						<xs:enumeration value="id-pe-qcStatements.id-etsi-qcs-QcSSCD"/>
					-->
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ExtensionValueTypeType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="BitMap"/>
			<xs:enumeration value="OID"/>
			<xs:enumeration value="String"/>
			<xs:enumeration value="Blob"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ExtensionEnumerationValueListType">
		<xs:sequence>
			<xs:element name="ExtensionEnumerationValue" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ExtensionEnumerationHexValue" type="xs:string" minOccurs="0"/>
						<xs:element name="ExtensionEnumerationLabelValue" type="xs:string" minOccurs="0"/>
						<xs:element name="ExtensionEnumerationLabel" type="xs:string" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExtensionTypeType">
		<xs:sequence>
			<xs:element name="ExtensionValueType" type="ExtensionValueTypeType"/>
			<xs:element name="ExtensionEnumerationValueList" type="ExtensionEnumerationValueListType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExtensionAliasType">
		<xs:sequence>
			<xs:element name="Identifier" type="ExtensionIdentifierType"/>
			<xs:element name="OID" type="xs:string" minOccurs="0"/>
			<xs:element name="FriendlyName" type="xs:string" minOccurs="0"/>
			<xs:element name="ExtensionType" type="ExtensionTypeType" minOccurs="0"/>
			<xs:element name="AliasList" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Alias" maxOccurs="unbounded">
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:token">
										<xs:attribute name="Id" type="xs:ID"/>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ExtensionAlias" type="ExtensionAliasType"/>
	<xs:complexType name="ExtensionAliasListType">
		<xs:sequence>
			<xs:element ref="ExtensionAlias" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ExtensionAliasList" type="ExtensionAliasListType"/>
	<xs:complexType name="KnownExtensionType">
		<xs:sequence>
			<xs:element name="Identifier" type="ExtensionIdentifierType">
				<xs:annotation>
					<xs:documentation/>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExtensionMapList" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ExtensionMap" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Value">
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="xs:token"/>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element ref="AliasRef"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="KnownExtension" type="KnownExtensionType"/>
	<xs:complexType name="KnownExtensionListType">
		<xs:sequence>
			<xs:element ref="KnownExtension" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="KnownExtensionList" type="KnownExtensionListType"/>
	<xs:complexType name="ApprovedCAType">
		<xs:sequence>
			<xs:element ref="Label"/>
			<xs:element ref="IsActive"/>
			<xs:element ref="IsQualified" minOccurs="0"/>
			<xs:element ref="QualificationContext" minOccurs="0"/>
			<xs:element name="ListedForTPConfig" type="zeroOuUnType"/>
			<xs:element ref="X509SubjectName"/>
			<xs:element ref="X509Certificate"/>
			<xs:sequence>
				<xs:choice>
					<xs:sequence>
						<xs:element ref="CRLDPList" minOccurs="0"/>
						<xs:element ref="DCRLDPList" minOccurs="0"/>
						<xs:element name="CRLUpdateFrequency" type="xs:integer" minOccurs="0"/>
						<xs:element name="DCRLUpdateFrequency" type="xs:integer" minOccurs="0"/>
						<xs:element name="CRLStartUpdateTime" type="xs:time" minOccurs="0"/>
						<xs:element name="DCRLStartUpdateTime" type="xs:time" minOccurs="0"/>
						<xs:element name="CRLUpdateFrequencyOnError" type="xs:integer" minOccurs="0"/>
						<xs:element name="DCRLUpdateFrequencyOnError" type="xs:integer" minOccurs="0"/>
						<xs:element name="BeforeExpirationCRLUpdateDeadline" type="xs:integer" minOccurs="0"/>
						<xs:element name="BeforeExpirationDCRLUpdateDeadline" type="xs:integer" minOccurs="0"/>
						<xs:element name="BusinessExpirationDelay" minOccurs="0">
							<xs:complexType>
								<xs:choice>
									<xs:element name="FromCRLCreation"/>
									<xs:element name="FromCRLDownload"/>
									<xs:element name="FromSynchronisationTime"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
						<xs:element name="MaxDeltaCRLCount" type="xs:integer" minOccurs="0"/>
						<xs:element ref="SrvAuthCert" minOccurs="0"/>
						<xs:element name="ActivateCRLRemoval" type="zeroOuUnType" minOccurs="0"/>
					</xs:sequence>
					<xs:element name="NoAssociatedCRL" type="xs:boolean"/>
				</xs:choice>
				<xs:element ref="OCSPResponderRefList" minOccurs="0"/>
			</xs:sequence>
			<xs:element ref="KnownExtensionList" minOccurs="0"/>
			<xs:element ref="AdminGroups" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Id" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:element name="ApprovedCA" type="ApprovedCAType"/>
	<xs:complexType name="ApprovedCAListType">
		<xs:sequence>
			<xs:element ref="ApprovedCA" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ApprovedCAList" type="ApprovedCAListType"/>
	<!--Configuration de la liste des serveurs OCSP disponibles-->
	<xs:element name="OCSPResponderRef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="OCSPResponderRefList" type="OCSPResponderRefListType"/>
	<xs:complexType name="OCSPResponderRefListType">
		<xs:sequence>
			<xs:element ref="OCSPResponderRef" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OCSPResponderType">
		<xs:sequence>
			<xs:element ref="Label"/>
			<xs:element ref="IsActive"/>
			<xs:element ref="URL"/>
			<xs:element ref="X509Certificate"/>
			<xs:element ref="PolicyIdentifier" minOccurs="0"/>
			<xs:element ref="SrvAuthCert" minOccurs="0"/>
			<xs:element ref="CryptoKeyRef" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Id" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:element name="OCSPResponder" type="OCSPResponderType"/>
	<xs:complexType name="OCSPResponderListType">
		<xs:sequence>
			<xs:element ref="OCSPResponder" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="OCSPResponderList" type="OCSPResponderListType"/>
	<!--Configuration de la liste des serveurs d'horodatage disponibles-->
	<xs:element name="TimeStampServerRef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="TimeStampServerRefListType">
		<xs:sequence>
			<xs:element ref="TimeStampServerRef" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TimeStampServerRefList" type="TimeStampServerRefListType"/>
	<xs:complexType name="TimeStampServerInstanceType">
		<xs:sequence>
			<xs:element name="InstanceLabel" type="xs:string" minOccurs="0"/>
			<xs:element name="Priority" type="xs:integer"/>
			<xs:element name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="RFC3161"/>
						<xs:enumeration value="EPM"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="URL"/>
			<xs:choice>
				<xs:element ref="X509Certificate" minOccurs="0"/>
				<xs:element name="X509CertificateAC" minOccurs="0"/>
			</xs:choice>
			<xs:element ref="PolicyIdentifier" minOccurs="0"/>
			<xs:element ref="SrvAuthCert" minOccurs="0"/>
			<xs:element name="CryptoKeyRef" minOccurs="0">
				<xs:complexType>
					<xs:attribute ref="Reference" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="DigestMethod" type="DigestMethodType" minOccurs="0"/>
			<xs:element name="Timeout" type="xs:integer"/>
			<xs:element name="NbRetry" type="xs:integer"/>
			<xs:element name="LockDuration" type="xs:integer"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimeStampServerInstanceListType">
		<xs:sequence>
			<xs:element name="TimeStampServerInstance" type="TimeStampServerInstanceType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TimeStampServerType">
		<xs:sequence>
			<xs:element ref="Label"/>
			<xs:element ref="IsActive"/>
			<xs:choice>
				<xs:sequence>
					<xs:element name="Type">
						<xs:simpleType>
							<xs:restriction base="xs:token">
								<xs:enumeration value="RFC3161"/>
								<xs:enumeration value="EPM"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element ref="URL"/>
					<xs:choice>
						<xs:element ref="X509Certificate" minOccurs="0"/>
						<xs:element name="X509CertificateAC" minOccurs="0"/>
					</xs:choice>
					<xs:element ref="PolicyIdentifier" minOccurs="0"/>
					<xs:element ref="SrvAuthCert" minOccurs="0"/>
					<xs:element name="CryptoKeyRef" minOccurs="0">
						<xs:complexType>
							<xs:attribute ref="Reference" use="required"/>
						</xs:complexType>
					</xs:element>
					<xs:element ref="AdminGroups" minOccurs="0"/>
					<xs:element name="DigestMethod" type="DigestMethodType" minOccurs="0"/>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="TimeStampServerInstanceList" type="TimeStampServerInstanceListType"/>
					<xs:element ref="AdminGroups" minOccurs="0"/>
				</xs:sequence>
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="Id" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:element name="TimeStampServer" type="TimeStampServerType"/>
	<xs:complexType name="TimeStampServerListType">
		<xs:sequence>
			<xs:element ref="TimeStampServer" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="TimeStampServerList" type="TimeStampServerListType"/>
	<!-- Configuration des homologues NTP autorisés (DTSS uniquement) -->
	<xs:complexType name="NtpPeerType">
		<xs:attribute name="Address" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:element name="NtpPeer" type="NtpPeerType"/>
	<xs:complexType name="NtpPeerListType">
		<xs:sequence>
			<xs:element ref="NtpPeer" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="Corum" type="xs:integer"/>
	</xs:complexType>
	<xs:element name="NtpPeerList" type="NtpPeerListType"/>
	<!--Configuration de la liste des moteurs cryptographiques-->
	<xs:element name="CryptoEngineInfoRef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TransportKeyRef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="CryptoEngineInfoType">
		<xs:sequence>
			<xs:element ref="Label"/>
			<xs:element name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="SOFT"/>
						<xs:enumeration value="PKCS11"/>
						<xs:enumeration value="PAYSHIELD"/>
						<xs:enumeration value="CRYPT2PAY"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="IsActive"/>
			<xs:element name="Localisation" type="xs:token"/>
			<xs:element ref="AdminGroups" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Id" type="xs:ID" use="required"/>
	</xs:complexType>
	<xs:element name="CryptoEngineInfo" type="CryptoEngineInfoType"/>
	<xs:complexType name="CryptoEngineInfoListType">
		<xs:sequence>
			<xs:element ref="CryptoEngineInfo" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CryptoEngineInfoList" type="CryptoEngineInfoListType"/>
	<!--Configuration de la liste des cles cryptographiques-->
	<xs:complexType name="CryptoKeyType">
		<xs:sequence>
			<xs:element name="Label" minOccurs="0"/>
			<xs:element name="UsingMode" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="AUTO-GENERATED"/>
						<xs:enumeration value="PERSISTENT"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Type">
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="SOFT"/>
						<xs:enumeration value="HSM-KEY-ONLY"/>
						<xs:enumeration value="HSM-KEY-AND-CRT"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="IsActive"/>
			<xs:element name="UsageList">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Usage" maxOccurs="3">
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="DOCUMENT-SIGNATURE"/>
									<xs:enumeration value="PROOF-SEALING"/>
									<xs:enumeration value="TECHNICAL"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:sequence>
				<xs:element ref="CryptoEngineInfoRef" minOccurs="0"/>
				<xs:element ref="TransportKeyRef" minOccurs="0"/>
				<xs:element name="Localisation" type="xs:token" minOccurs="0"/>
			</xs:sequence>
			<xs:element ref="X509SubjectName" minOccurs="0"/>
			<xs:element ref="X509Certificate" minOccurs="0"/>                        
			<xs:element name="PinSession" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
