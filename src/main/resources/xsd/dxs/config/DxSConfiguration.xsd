				<xs:element name="WantPrivateKeyIfAutoGenerated" type="zeroOuUnType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--Politique de confiance AxS-->
	<xs:element name="QualificationContext" type="xs:string"/>
	<xs:element name="IsQualified" type="zeroOuUnType"/>
	<xs:element name="TrustPolicyRef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="TrustPolicyType">
		<xs:sequence>
			<xs:element ref="Label"/>
			<xs:element ref="IsActive"/>
			<xs:element ref="IsQualified"/>
			<xs:element ref="QualificationContext" minOccurs="0"/>
			<xs:element ref="MaxDocumentSize"/>
			<xs:element ref="ExpirationPolicyRef" minOccurs="0"/>
			<xs:choice>
				<xs:element ref="SignatureValidation"/>
				<xs:element ref="CertificateValidation"/>
				<xs:element ref="SignatureCreation"/>
				<xs:element ref="DataDecryption"/>
				<xs:element ref="AuthenticationValidation"/>
			</xs:choice>
			<xs:element ref="ProofCreation"/>
			<xs:element ref="ProofArchival"/>
			<xs:element ref="Response"/>
			<xs:element ref="PluginCallerList" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Id" type="xs:ID" use="required"/>
	</xs:complexType>
	<!--Applications metiers et transaction-->
	<xs:element name="TransactionList">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Transaction" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Label"/>
							<xs:element ref="TrustPolicyRef"/>
							<xs:element name="Description" type="xs:token" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="OCSPTransactionList">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="OCSPTransaction" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Label"/>
							<xs:element ref="TrustPolicyRef"/>
							<xs:element name="Description" type="xs:token" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="ApplicationType">
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element ref="Label"/>
					<xs:element ref="X509SubjectName" minOccurs="0"/>
					<xs:element ref="X509Certificate"/>
                                        <xs:element name="X509CertificateList" type="X509CertificateListType" minOccurs="0" />
				</xs:sequence>
				<xs:element name="ApplicationMultiGroupRef">
					<xs:complexType>
						<xs:attribute ref="Reference" use="required"/>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element ref="IsActive" minOccurs="0"/>
			<xs:element ref="TransactionList"/>
			<xs:element ref="OCSPTransactionList" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Application" type="ApplicationType"/>
	<xs:complexType name="ApplicationListType">
		<xs:sequence>
			<xs:element ref="Application" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ApplicationList" type="ApplicationListType"/>
	<!--Groupes d'administration-->
	<xs:element name="GroupRef">
		<xs:complexType>
			<xs:attribute ref="Reference" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="Group">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Label"/>
				<xs:element ref="IsActive"/>
				<xs:element ref="UserList"/>
				<xs:element ref="AuthenticationKeyList" minOccurs="0"/>
				<xs:element ref="ProvisioningPolicyList" minOccurs="0"/>
				<xs:element ref="ExpirationPolicyList" minOccurs="0"/>
				<xs:element name="TrustPolicyList">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="TrustPolicy" type="TrustPolicyType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="ApplicationList"/>
				<xs:element ref="Seal"/>
			</xs:sequence>
			<xs:attribute name="Id" type="xs:ID" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="GroupList">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Group" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--Niveau "Administration" des fichiers de configuration A2S et AVS-->
	<xs:complexType name="DVSAdministrationType">
		<xs:sequence>
			<xs:element ref="GroupList"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="D2SAdministrationType">
		<xs:sequence>
			<xs:element ref="GroupList"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DTSSAdministrationType">
		<xs:sequence>
			<xs:element ref="GroupList"/>
		</xs:sequence>
	</xs:complexType>
	<!--**********************************************************************************************************-->
	<!--FORMAT DU FICHIER DE CONFIGURATION AVS-->
	<!--**********************************************************************************************************-->
	<xs:complexType name="DVSConfigurationType">
		<xs:sequence>
			<xs:element ref="SuperAdministration"/>
			<xs:element name="Administration" type="DVSAdministrationType"/>
			<xs:element ref="Seal"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DVSConfiguration" type="DVSConfigurationType" substitutionGroup="DxSConfiguration"/>
	<!--**********************************************************************************************************-->
	<!--FORMAT DU FICHIER DE CONFIGURATION A2S-->
	<!--**********************************************************************************************************-->
	<xs:complexType name="D2SConfigurationType">
		<xs:sequence>
			<xs:element ref="SuperAdministration"/>
			<xs:element name="Administration" type="D2SAdministrationType"/>
			<xs:element ref="Seal"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="D2SConfiguration" type="D2SConfigurationType" substitutionGroup="DxSConfiguration"/>
	<!--**********************************************************************************************************-->
	<!--FORMAT DU FICHIER DE CONFIGURATION DTSS-->
	<!--**********************************************************************************************************-->
	<xs:complexType name="DTSSConfigurationType">
		<xs:sequence>
			<xs:element ref="SuperAdministration"/>
			<xs:element name="Administration" type="DTSSAdministrationType"/>
			<xs:element ref="Seal"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="DTSSConfiguration" type="DTSSConfigurationType" substitutionGroup="DxSConfiguration"/>
	<!--**********************************************************************************************************-->
	<!--FORMAT DU FICHIER DE CONFIGURATION AxS REGROUPANT LES  CONFIGURATION AVS et A2S-->
	<!--**********************************************************************************************************-->
	<xs:element name="DxSConfiguration" abstract="true">
		<xs:annotation>
			<xs:documentation source="www.dictao.com"/>
		</xs:annotation>
	</xs:element>
        <!-- BDF Type -->
        <xs:complexType name="X509CertificateListType">
		<xs:sequence>
                    <xs:element name="X509CertificateItem" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="X509SubjectName" type="xs:string"/>
                                <xs:element name="X509IssuerName" type="xs:string"/>
                                <xs:element name="X509CreationDate" type="xs:dateTime"/>
                                <xs:element name="X509ExpirationDate" type="xs:dateTime"/>
                                <xs:element name="X509Sha1" type="xs:string"/>  
                                <xs:element name="X509Certificate" type="xs:string"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
